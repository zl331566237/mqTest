target 'mtqqPlus' do

  pod 'AFNetworking'#网络请求框架
  pod 'MBProgressHUD'
  pod 'Masonry'
  pod 'MJExtension'
  pod 'LYEmptyView'
  pod 'YBPopupMenu'
  #pod 'CocoaAsyncSocket'
  # MQTT
  pod 'MQTTClient'


end

